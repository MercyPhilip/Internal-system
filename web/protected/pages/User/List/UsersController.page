<com:TContent ID="MainContent">
	<com:Application.controls.fancybox.FancyBox />
	<com:TCallback ID="getUsersBtn" OnCallback="Page.getUsers" />
	<com:TCallback ID="deleteUserBtn" OnCallback="Page.deleteUser" />
	<com:TCallback ID="getRolesBtn" OnCallback="Page.getRoles" />
	<div class="panel panel-default">
		<div class="panel-heading">
			Search/Add Users:
			<div class="pull-right">
				<input id="addBtn" type="button" value="Add User" class="btn btn-success" onclick="pageJs._showEditPanel()" />
			</div>
		</div>
		<div class="panel-body">
			<span id="totalUsers">0</span> Users found:
		</div>
		<div class="list-group" id='resultDiv'></div>
	</div>
</com:TContent>